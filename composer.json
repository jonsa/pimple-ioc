{
    "name": "jonsa/pimple-ioc",
    "description": "Resolve classes out of a Pimple container",
    "keywords": ["pimple", "ioc"],
    "type": "library",
    "require": {
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Sandstrom",
            "email": "jonas@sandstrom.im"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jonsa\\PimpleResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonsa\\PimpleResolver\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
